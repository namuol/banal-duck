user-select()
  -webkit-touch-callout arguments
  -webkit-user-select arguments
  -khtml-user-select arguments
  -moz-user-select arguments
  -ms-user-select arguments
  -o-user-select arguments
  user-select arguments

text-shadow()
  -webkit-text-shadow arguments
  -moz-text-shadow arguments
  text-shadow arguments

linear-gradient(a,b)
  -webkit-gradient(linear,left top,left bottom,color-stop(0, a),color-stop(1, b))

body
  height 100%
  width 100%
  user-select none
  color #fff
  text-shadow 0 1px 2px rgba(0,0,0,0.8)
  background-color #6995ac
  background -webkit-radial-gradient(transparent 0%, rgba(0,0,0,0.4)), url('bg.png')
  background -moz-radial-gradient(transparent 0%, rgba(0,0,0,0.4)), url('bg.png')
  background -o-radial-gradient(transparent 0%, rgba(0,0,0,0.4)), url('bg.png')
  border 0
  padding 0
  margin 0
  font-family Helvetica, sans-serif, Arial

#layout
  height 100%
  width 100%

#layout td
  text-align center
  width 34%

#grid
  border-collapse collapse
  margin 0 auto

#grid_wrap
  width 453px
  min-width 453px

#grid td
  border 1px solid #888
  width 150px
  height 150px

#a_button, #b_button
  color #888
  font-weight bold

#grid td.highlighted
  background #369

.success
  background #afa

.failure
  background #faa

#hud
  font-weight bold
  height 30px

#type
  float left
#remaining
  float right

#results
  text-shadow 0px 1px 3px rgba(0, 0, 0, 0.9)
  display inline-block
  color #ccc
  width 100%
  height 100%
  background rgba(0,0,0,0.75)
  top 0
  left 0
  position absolute
  text-align center

#instructions
  text-align left
  font-size 18pt

#start_game_msg
  width 100%
  text-align center
  position absolute
  bottom 15px
  font-size 24pt

#graph
  width 400px
  height 200px
  margin-left 20px

#nav
  overflow hidden
  width 100%
  height 44px

#nav > ul
  margin 0
  padding 0

#nav > ul > li
  border-radius 0 !important
  border 0 !important
  font-size 14pt
  font-weight bold
  text-align center
  float left
  width 33%
  padding-top 10px
  height 30px

#nav > ul > li.first
  padding-left 1%

#nav a
  color #fff
  text-decoration none

#nav li a:hover
  cursor default

#nav li.selected, #nav li.selected:hover
  cursor auto

  background linear-gradient(#c84, #930)
  background -webkit-linear-gradient(#c84, #930)
  background -moz-linear-gradient(#c84, #930)
  background -o-linear-gradient(#c84, #930)
  box-shadow rgba(0,0,0,0.5) 0 2px 6px,rgba(0,0,0,0.5) 0 2px 6px inset

#game
  padding 20px

legend
  color rgba(0,0,0,0.5)
  float left
  font-size 15pt
  padding 0
  padding-bottom 10px
  margin 0
  margin-top -35px
  text-shadow 0 1px 0 rgba(255,255,255,0.8), 0 0px 0 rgba(0,0,0,0.8)

#game input[type='number']
  font-size 14pt
  width 6em
  margin-bottom 10px
  border 0
  background none
  color #fff
  text-align right
  text-shadow 0 1px 0 rgba(0,0,0,0.8)

#game-mode, #settings
  float left
  height 164px
  width 200px

#new-game
  //text-shadow 0 1px 0 rgba(255,255,255,0.8)
  float left
  font-size 18pt
  font-weight bold
  width 200px
  height 50px

input[type='radio']
  display none

input[type='radio'] + label
  display block
  margin 10px
  padding 6px
  height 27px
  font-size 16pt
  text-align center

input[type='radio']:checked + label, input[type='radio']:checked + label:hover
  cursor default
  background linear-gradient(#c84, #930)
  background -webkit-linear-gradient(#c84, #930)
  background -moz-linear-gradient(#c84, #930)
  background -o-linear-gradient(#c84, #930)
  box-shadow rgba(0,0,0,0.5) 0 2px 6px inset

button, input[type='radio'] + label, #nav > ul > li
  color #fff
  border 1px solid #000
  border-radius 10px
  background linear-gradient(#567, #17191E)
  background -webkit-linear-gradient(#567, #17191E)
  background -moz-linear-gradient(#567, #17191E)
  background -o-linear-gradient(#567, #17191E)
  box-shadow rgba(255, 255, 255, 0.25) 0 1px 0 inset, rgba(0,0,0,0.5) 0 2px 6px
  text-shadow 0 -1px 0 rgba(0,0,0,0.8)

button:hover, input[type='radio'] + label:hover, #nav > ul > li:hover
  //cursor pointer
  background linear-gradient(#789, #17191E)
  background -webkit-linear-gradient(#789, #17191E)
  background -moz-linear-gradient(#789, #17191E)
  background -o-linear-gradient(#789, #17191E)

button:active, input[type='radio'] + label:active, #nav > ul > li:active
  background linear-gradient(#567, #17191E)
  background -webkit-linear-gradient(#567, #17191E)
  background -moz-linear-gradient(#567, #17191E)
  background -o-linear-gradient(#567, #17191E)
  box-shadow rgba(0,0,0,0.5) 0 2px 6px

fieldset
  display inline-block
  border-radius 5px
  border 1px solid #000
  background linear-gradient(#abc, #789)
  background -webkit-linear-gradient(#abc, #789)
  background -moz-linear-gradient(#abc, #789)
  background -o-linear-gradient(#abc, #789)
  box-shadow rgba(0,0,0,0.5) 0 1px 2px inset
  color rgba(0,0,0, 0.75)
  font-weight bold
  text-shadow 0 1px 0 rgba(255,255,255,0.48)
  padding 5px
  margin 5px
  margin-top 35px

fieldset.outer
  background linear-gradient(#cdf, #abc)
  background -webkit-linear-gradient(#cdf, #abc)
  background -moz-linear-gradient(#cdf, #abc)
  background -o-linear-gradient(#cdf, #abc)
  box-shadow rgba(255, 255, 255, 0.25) 0 1px 0 inset, rgba(0,0,0,0.5) 0 2px 6px
  margin-top 0px

#help
  padding 20px
